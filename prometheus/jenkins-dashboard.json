{
  "dashboard": {
    "id": null,
    "uid": "jenkins",
    "title": "Jenkins Monitoring",
    "tags": ["jenkins", "ci/cd", "monitoring"],
    "timezone": "browser",
    "schemaVersion": 38,
    "version": 1,
    "style": "dark",
    "editable": true,
    "hideControls": false,
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m"],
      "time_options": ["5m", "15m", "1h", "6h", "12h", "24h"]
    },
    "templating": {
      "list": [
        {
          "name": "DS_PROMETHEUS",
          "type": "datasource",
          "label": "Prometheus",
          "query": "prometheus",
          "refresh": 1
        }
      ]
    },
    "annotations": {
      "list": [
        {
          "name": "Annotations",
          "type": "dashboard",
          "enable": true
        }
      ]
    },
    "panels": [
      {
        "type": "text",
        "title": "Jenkins Overview",
        "gridPos": { "x": 0, "y": 0, "w": 24, "h": 4 },
        "options": {
          "content": "<h2>Jenkins Monitoring Dashboard</h2><p><a href='https://www.jenkins.io/' target='_blank'>Jenkins Docs</a> | <a href='https://github.com/jenkinsci' target='_blank'>GitHub</a></p>",
          "mode": "html"
        }
      },
      {
        "type": "stat",
        "title": "Build Queue Size",
        "datasource": "$DS_PROMETHEUS",
        "targets": [
          {
            "expr": "jenkins_queue_size",
            "refId": "A"
          }
        ],
        "gridPos": { "x": 0, "y": 4, "w": 6, "h": 6 }
      },
      {
        "type": "stat",
        "title": "Build Executor Usage",
        "datasource": "$DS_PROMETHEUS",
        "targets": [
          {
            "expr": "jenkins_executor_utilization",
            "refId": "A"
          }
        ],
        "gridPos": { "x": 6, "y": 4, "w": 6, "h": 6 }
      },
      {
        "type": "time_series",
        "title": "Builds in Progress",
        "datasource": "$DS_PROMETHEUS",
        "targets": [
          {
            "expr": "jenkins_builds_in_progress",
            "refId": "A"
          }
        ],
        "gridPos": { "x": 12, "y": 4, "w": 12, "h": 9 }
      },
      {
        "type": "time_series",
        "title": "Build Duration (seconds)",
        "datasource": "$DS_PROMETHEUS",
        "targets": [
          {
            "expr": "rate(jenkins_builds_duration_sum[5m]) / rate(jenkins_builds_duration_count[5m])",
            "refId": "A"
          }
        ],
        "gridPos": { "x": 0, "y": 10, "w": 12, "h": 9 }
      },
      {
        "type": "time_series",
        "title": "Failed Builds",
        "datasource": "$DS_PROMETHEUS",
        "targets": [
          {
            "expr": "increase(jenkins_builds_failed_total[1h])",
            "refId": "A"
          }
        ],
        "gridPos": { "x": 12, "y": 10, "w": 12, "h": 9 }
      }
    ]
  },
  "overwrite": true
}
