{
  "dashboard": {
    "id": null,
    "uid": "prometheus-overview",
    "title": "Prometheus Overview",
    "tags": ["prometheus", "monitoring"],
    "timezone": "browser",
    "schemaVersion": 38,
    "version": 1,
    "style": "dark",
    "editable": true,
    "time": {
      "from": "now-6h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m"],
      "time_options": ["5m", "15m", "1h", "6h", "12h", "24h"]
    },
    "templating": {
      "list": [
        {
          "name": "prometheus",
          "type": "datasource",
          "query": "prometheus",
          "label": "Prometheus",
          "refresh": 1
        }
      ]
    },
    "annotations": {
      "list": []
    },
    "panels": [
      {
        "type": "stat",
        "title": "Prometheus CPU Usage",
        "datasource": "$prometheus",
        "targets": [
          {
            "expr": "process_cpu_seconds_total",
            "refId": "A"
          }
        ],
        "gridPos": { "x": 0, "y": 0, "w": 6, "h": 4 }
      },
      {
        "type": "stat",
        "title": "Prometheus Memory Usage",
        "datasource": "$prometheus",
        "targets": [
          {
            "expr": "process_resident_memory_bytes",
            "refId": "A"
          }
        ],
        "gridPos": { "x": 6, "y": 0, "w": 6, "h": 4 }
      },
      {
        "type": "stat",
        "title": "Prometheus Uptime",
        "datasource": "$prometheus",
        "targets": [
          {
            "expr": "time() - process_start_time_seconds",
            "refId": "A"
          }
        ],
        "gridPos": { "x": 12, "y": 0, "w": 6, "h": 4 }
      },
      {
        "type": "time_series",
        "title": "Active Time Series",
        "datasource": "$prometheus",
        "targets": [
          {
            "expr": "prometheus_tsdb_head_series",
            "refId": "A"
          }
        ],
        "gridPos": { "x": 0, "y": 4, "w": 12, "h": 6 }
      },
      {
        "type": "time_series",
        "title": "Ingested Samples",
        "datasource": "$prometheus",
        "targets": [
          {
            "expr": "rate(prometheus_tsdb_head_samples_appended_total[5m])",
            "refId": "A"
          }
        ],
        "gridPos": { "x": 12, "y": 4, "w": 12, "h": 6 }
      },
      {
        "type": "time_series",
        "title": "Checkpoint Duration",
        "datasource": "$prometheus",
        "targets": [
          {
            "expr": "prometheus_tsdb_checkpoint_duration_seconds",
            "refId": "A"
          }
        ],
        "gridPos": { "x": 0, "y": 10, "w": 12, "h": 6 }
      },
      {
        "type": "time_series",
        "title": "Notifications Queue Length",
        "datasource": "$prometheus",
        "targets": [
          {
            "expr": "prometheus_notifications_queue_length",
            "refId": "A"
          }
        ],
        "gridPos": { "x": 12, "y": 10, "w": 12, "h": 6 }
      }
    ]
  },
  "overwrite": true
}
